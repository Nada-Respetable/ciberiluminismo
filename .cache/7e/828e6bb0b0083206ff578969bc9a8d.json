\n    h2,h3,h4{\r\n        padding-bottom: $space-unit;\r\n        padding-top: $space-unit;\r\n    }\r\n    h1,h2,h3,h4{\r\n        font-family: $title-font;\r\n        font-weight: bold;\r\n        text-transform: uppercase;\r\n        text-align: center;\r\n    }\r\np{\r\n    padding-bottom: $space-unit;\r\n} \r\na {\r\n    &:visited{\r\n        color: grey;\r\n    }\r\n} \r\nimg {\r\n    padding-top: $space-unit;\r\n    padding-bottom: $space-unit*2;\r\n    display: block;\r\n    margin: 0 auto;\r\n    max-width: 400px;\r\n}\r\n.caption{\r\n    text-align: center;\r\n    font-size: $space-unit;\r\n    opacity: 0.5;\r\n    padding-bottom: $space-unit*2;\r\n}\r\nblockquote{\r\n    font-size: $space-unit*1.6;\r\n    text-align: center;\r\n    padding-top: $space-unit;\r\n}\r\ncite{\r\n    display: block;\r\n    font-size: $space-unit;\r\n    font-style: normal;\r\n    opacity: 0.5;\r\n    text-align: center;\r\n    padding-top: $space-unit;\r\n    padding-bottom: $space-unit*2;\r\n} \r\nul{\r\n    list-style-position: inside; } }\r\nfooter{\r\n    margin-top: $space-unit*5;\r\n    padding-bottom: $space-unit*1.5;\r\n    font-size: $space-unit;\r\n    text-align: center;\r\n}\r\n@media(max-width: 800px){\r\n    footer{\r\n        margin-left: $space-unit*2;\r\n        margin-right: $space-unit*2;\r\n        padding-bottom: $space-unit*2;\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"90080921531d590593ac417ed914c8fa","cacheData":{"env":{}}}